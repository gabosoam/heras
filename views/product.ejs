                        <hr>
                        <table id="Vouchers" class="table ">
                            <thead style="font-size:13px;">
                                <tr>
                                    <th style="text-align: center">Ítem</th>
                                    <th style="text-align: center">Descripción</th>
                                    <th style="text-align: center">Código</th>
                                    <th style="text-align: center">Serie</th>
                                    <th style="text-align: center">Cant.</th>
                                    <th style="text-align: center">Observación</th>
                                </tr>
                            </thead>
                            <tbody style="font-size:11px;">
                                <tr w3-repeat="voucherss">
                                    <td style="text-align: center">{{num}}</td>
                                    <td style="text-align: center">{{description}}</td>
                                    <td style="text-align: center">{{code}}</td>
                                    <td style="text-align: center">{{barcode}}</td>
                                    <td style="text-align: center">{{count}}</td>
                                    <td style="text-align: center">
                                        {{observation}}
                                    </td>
                                </tr>


                            </tbody>
                        </table>
                        <hr>

                        <div class="well m-t">
                            <strong>Comentario</strong>
                            <input class="form-control" type="text">
                        </div>



                        <table style="width: 100%; border: 1; font-size: 12px ">
                            <tr>
                                <td>
                                    <center>
                                        <p>
                                            ENTREGADO POR
                                            <br>
                                            <br>
                                            <br>
                                            <br>
                                            <br> _______________________________
                                            <br>
                                            <br>
                                            <input type="text" style="border: 0; width: 100%; text-align: center; text-transform: uppercase">
                                        </p>
                                    </center>


                                </td>

                                <td>
                                    <center>
                                        <p>
                                            REPRESENTANTE CINETO
                                            <br>
                                            <br>
                                            <br>
                                            <br>
                                            <br> _______________________________
                                            <br>
                                            <br>

                                        </p>
                                    </center>


                                </td>

                                <td>
                                    <center>
                                        <p>
                                            RECIBIDO POR
                                            <br>
                                            <br>
                                            <br>
                                            <br>
                                            <br> _______________________________
                                            <br>
                                            <br>
                                            <input type="text" style="border: 0; width: 100%; text-align: center; text-transform: uppercase">
                                        </p>
                                    </center>


                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="myModalLotes" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Ingreso por lotes</h4>
                </div>
                <div class="modal-body">
                    <a class="btn btn-link" href="../PLANTILLA.csv"> Descargar plantilla</a>
                    <br>
                    <br>
                    <form class="form-horizontal ">

                        <fieldset>
                            <label for="csvFileInput">
                                <strong>Seleccionar archivo CSV:</strong>
                            </label>
                            <input type="file" class="" id="csvFileInput" onchange="handleFiles(this.files)" accept=".csv">
                            <br>
                            <!-- <i id="spinner" class="fa fa-spinner fa-spin fa-2x" aria-hidden="true"></i> -->

                        </fieldset>
                    </form>

                </div>
                <div class="tabs-container">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a data-toggle="tab" href="#tab-1"> Productos</a>
                        </li>
                        <li class="">
                            <a data-toggle="tab" href="#tab-2">Nuevos códigos</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div id="tab-1" class="tab-pane active">
                            <div class="panel-body">
                                <button class="btn btn-primary" onclick="iniciar()">Iniciar ingreso</button>
                                <div id="output2" style="height: 150; background-color: white"></div>
                            </div>
                        </div>
                        <div id="tab-2" class="tab-pane">
                            <div class="panel-body">
                                <button class="btn btn-primary" onclick="change()">Cambiar</button>
                                <div id="output" style="height: 150; background-color: white"></div>
                            </div>
                        </div>
                    </div>


                </div>
            </div>

        </div>
    </div>

    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Registrar modelo</h4>
                </div>
                <div class="modal-body">
                    <form id="formSaveModel">


                        <div class="form-group">
                            <label for="code">Código:</label>
                            <input autofocus type="text" class="form-control" validationMessage="Selecciona un producto" required id="codeModal" name="code"
                                style="width: 100%">
                        </div>


                        <div class="form-group">
                            <label for="pwd">Nombre del producto:</label>
                            <input type="text" class="form-control" validationMessage="Nombre" name="description" required id="description" style="width: 100%">
                        </div>




                        <div class="form-group">
                            <label for="pwd">Stock mín:</label>
                            <input type="number" validationMessage="Ingresa el stock mínimo" class="form-control" required name="stockmin" placeholder="Stock mínimo"
                                id="stockmin" style="width: 100%">
                        </div>




                        <div class="form-group">
                            <label for="pwd">Unidad de medida:</label>
                            <input type="text" class="form-control" required name="unit" id="unit" style="width: 100%">
                        </div>






                        <div class="form-group">
                            <label for="pwd">Categoría:</label>
                            <input type="text" class="form-control" required name="category" id="category" style="width: 100%">
                        </div>





                        <div class="form-group">
                            <label for="pwd">Marca:</label>
                            <input type="text" class="form-control" required name="brand" id="brand" style="width: 100%">
                        </div>


                </div>
                <div class="modal-footer">
                    <button id="saveModel" type="button" class="btn btn-primary">Guardar</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
                </form>
            </div>
        </div>
    </div>



